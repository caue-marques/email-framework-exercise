<!DOCTYPE html
  PUBLIC "-//W3C//DTD XHTML 1.0 Transitional //EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:v="urn:schemas-microsoft-com:vml">
<!-- partial:partials/html-head.hbs -->
<!-- partial -->

<body>
  <table id="body-table" width="100%" border="0" cellpadding="0" cellspacing="0" style="max-width: 640px">
    <tr>
      <td align="center" valign="top">
        <table width="640px" style="max-width: 640px">
          <tr height="11px"></tr>
          <tr>
            <td colspan="8" class="logo align-center" style="padding: 0; text-align: center">
            {{#eq orders.0.clientProfileData.lastName "Priolli"}}
              <img style="margin: 20px 0; width: 40%; height: 40%" src="https://s2.glbimg.com/v1AhzxLmQERiJF4nRAL1MM9kYHQ=/0x0:1280x853/924x0/smart/filters:strip_icc()/i.s3.glbimg.com/v1/AUTH_e84042ef78cb4708aeebdf1c68c6cbd6/internal_photos/bs/2020/1/0/66nFHyRRAGRSyGrGJuPg/creme-de-milho.jpeg" />
            {{else}}
              <img style="margin: 20px 0" src="https://i.ibb.co/4jDhTyp/logo-rihappy-2d-solzinho-2.png" />
            {{/eq}}
            </td>
          </tr>
          <tr colspan="7" width="100%">
            <td colspan="1" style="height: 4px; background-color: #EC008C"></td>
            <td colspan="1" style="height: 4px; background-color: #15BEF0"></td>
            <td colspan="1" style="height: 4px; background-color: #F26322"></td>
            <td colspan="1" style="height: 4px; background-color: #62489D"></td>
            <td colspan="1" style="height: 4px; background-color: #2CB34A"></td>
            <td colspan="1" style="height: 4px; background-color: #EE3124"></td>
            <td colspan="1" style="height: 4px; background-color: #F177AE"></td>
          </tr>
        </table>
        <tr>
          <td class="w-100 bb b--white bw3 tc">
          {{#each orders}}
          {{#compare @index '==' 0}}
            {{#richShippingData shippingData}}
            {{#eq logisticsInfo.0.selectedDeliveryChannel "pickup-in-point"}}
              <img class="w-80 bt b--white bw3 mt3" src="https://i.ibb.co/q1wwHcT/Frame-1600.png" />
            {{else}}
              <img class="w-80 bt b--white bw3 mt3" src="https://i.ibb.co/q1wwHcT/Frame-1600.png" />
            {{/eq}}
            {{/richShippingData}}
          {{/compare}}
          {{/each}}
          </td>
        </tr>
        <tr>
          <td class="w-100 pl4 pt2" style="color: #373737; font-size: 30px">
          {{#if orders}}
            {{#if orders.0.clientProfileData.firstName}}
            {{#compare orders.0.clientProfileData.firstName '!=' 'isAnonymous'}}
              <p><strong>((i18n.order.title)) {{orders.0.clientProfileData.firstName}} :)</strong></p>
            {{/compare}}
            {{/if}}
          {{else}}
            {{#if clientProfileData.firstName}}
            {{#compare clientProfileData.firstName '!=' 'isAnonymous'}}
              <p>((i18n.common.hi)) <strong>{{clientProfileData.firstName}}</strong>,</p>
            {{/compare}}
            {{/if}}
          {{/if}}
          </td>
        </tr>
        <tr>
          <td class="w-100 ph4 lh-copy" style="color: #373737; font-size: 15px">
            <p>((i18n.order.congratulations)) ((i18n.order.yourOrder)) <strong>{{orders.0.orderId}}</strong> ((i18n.order.introMessageReplaced))</p>
          </td>
        </tr>
        <tr>
          <td class="w-100 ph4 lh-copy" style="color: #373737; font-size: 15px">
            <p>((i18n.order.introMessage))</p>
          </td>
        </tr>
        <!-- partial:partials/line.hbs -->
        <!-- partial -->
        {{#each orders}}
        <tr>
          <td class="w-100 ph4 lh-copy">
          <!-- partial:partials/order-info.hbs -->
          <!-- partial -->
          </td>
        </tr>
        {{/each}}
        <!-- partial:partials/line.hbs -->
        <!-- partial -->
        {{#each orders}}
        <tr>
          <td class="w-100 ph4 lh-copy">
          <!-- partial:partials/order-summary.hbs -->
          <!-- partial -->
          </td>
        </tr>
        {{/each}}
        <!-- partial:partials/line.hbs -->
        <!-- partial -->
        <tr>
          <td>
          <!-- partial:partials/access-order-button.hbs -->
          <!-- partial -->
          </td>
        </tr>
        <tr class="w-100 ph4-ns mw6">
          <!-- partial:partials/footer.hbs -->
          <!-- partial -->
        </tr>
      </td>
    </tr>
  </table>
</body>
</html>